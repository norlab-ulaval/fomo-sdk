{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "packages": {
    ".": {
      "release-type": "python",
      "package-name": "fomo-sdk",
      "initial-version": "1.0.4"
    },
    "fomo_rust": {
      "release-type": "rust",
      "package-name": "fomo-rust",
      "initial-version": "1.0.4"
    }
  },
  "plugins": [
    {
      "type": "linked-versions",
      "groupName": "fomo-project",
      "components": ["fomo-sdk", "fomo-rust"]
    }
  ]
}
